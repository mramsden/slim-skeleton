{
	"name": "bitsden/slim-skeleton",
	"description": "A Slim Framework skeleton application for rapid development",
	"authors": [
		{
			"name": "Marcus Ramsden",
			"email": "marcus@bitsden.com"
		}
	],
	"require": {
		"php": ">=7.3",
		"slim/slim": "^3.12.1",
		"monolog/monolog": "^1.24.0"
	},
	"require-dev": {
		"phpunit/phpunit": ">=5.0"
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"config": {
		"process-timeout": 0,
		"sort-packages": true
	},
	"scripts": {
		"start": "php -s localhost:8080 -t public",
		"test": "phpunit"
	}
}